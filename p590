/*
// Definition for a Node.
class Node {
    public int val;
    public List<Node> children;

    public Node() {}

    public Node(int _val) {
        val = _val;
    }

    public Node(int _val, List<Node> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
    List<Integer> list;
    public List<Integer> postorder(Node root) {
        list = new LinkedList();
        dfs(root);
        if(root!=null){
            list.add(root.val);
        }
        return list;
    }
    public void dfs(Node root){
        if(root == null) return;
        for(Node temp:root.children){
            dfs(temp);
            list.add(temp.val);
        }
    }
}
